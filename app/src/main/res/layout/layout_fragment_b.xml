                tools:ignore="MissingPrefix" />

            <android.support.constraint.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="16dp"
                android:background="@color/colorPrimary"
                android:paddingBottom="8dp"
                card_view:layout_constraintLeft_toLeftOf="@id/morning_a"
                card_view:layout_constraintRight_toRightOf="@id/morning_a"
                card_view:layout_constraintTop_toBottomOf="@id/morning_a">

                <TextView
                    android:id="@+id/az"
                    fontPath="fonts/vazir.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="12dp"
                    android:text="از"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    card_view:layout_constraintRight_toRightOf="parent"
                    card_view:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingPrefix" />

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:alpha="0.5"
                    android:background="@color/trs"
                    card_view:cardCornerRadius="5dp"

                    card_view:cardElevation="5dp"
                    card_view:layout_constraintRight_toLeftOf="@id/az"
                    card_view:layout_constraintTop_toTopOf="@id/az">

                    <TextView
                        android:id="@+id/t1"
                        fontPath="fonts/vazir.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
