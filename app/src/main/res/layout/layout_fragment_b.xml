                        android:textColor="@color/black"
                        android:textSize="15sp"
                        tools:ignore="MissingPrefix" />
                </android.support.v7.widget.CardView>

                <TextView
                    android:id="@+id/ta1"
                    fontPath="fonts/vazir.ttf"
