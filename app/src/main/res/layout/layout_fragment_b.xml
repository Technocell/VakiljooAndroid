<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color"
    android:orientation="vertical">

    <TextView
        android:id="@+id/contact_office_hours_txt"
        fontPath="fonts/vazir.ttf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="ساعات حضور شما در دفتر کارتان"
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        android:textSize="14sp"
        tools:ignore="MissingPrefix" />


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/card_view_orange"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        card_view:cardCornerRadius="15dp"
        card_view:cardElevation="10dp"
        card_view:contentPaddingBottom="8dp"
        card_view:contentPaddingTop="8dp">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            android:background="@color/white">


            <TextView
                android:id="@+id/morning"
                fontPath="fonts/vazir.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               android:paddingRight="80dp"
                android:gravity="center_vertical"
                android:text="صبح"
                android:textColor="@color/colorPrimary"
                android:textSize="14sp"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingPrefix" />

            <TextView
                android:id="@+id/afternoon"
                fontPath="fonts/vazir.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="60dp"
                android:gravity="center_vertical"
                android:text="بعد از ظهر"
                android:textColor="@color/colorPrimary"
                android:textSize="14sp"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toTopOf="@id/morning"
                tools:ignore="MissingPrefix" />


            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:background="@color/white"
                android:paddingBottom="4dp"
                card_view:layout_constraintLeft_toLeftOf="@id/morning"
                card_view:layout_constraintRight_toRightOf="@id/morning"
                card_view:layout_constraintTop_toBottomOf="@id/morning">
                <ImageView
                    android:id="@+id/calander"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginRight="4dp"
                    card_view:layout_constraintRight_toRightOf="parent"
                    card_view:layout_constraintTop_toTopOf="parent"
                    android:src="@drawable/calendar"/>

                <TextView
                    android:id="@+id/az_y"
                    fontPath="fonts/vazir.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:paddingTop="4dp"
                    android:text="از"
                    android:paddingRight="20dp"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp"
                   card_view:layout_constraintRight_toLeftOf="@id/calander"
                    card_view:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingPrefix" />

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view_y"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:background="@color/background_color"
                    android:backgroundTint="@color/cadr_cardview"
                    card_view:cardCornerRadius="4dp"

                    card_view:cardElevation="4dp"
                    card_view:layout_constraintRight_toLeftOf="@id/az_y"
                    card_view:layout_constraintTop_toTopOf="@id/az_y">

                    <TextView
                        android:id="@+id/t1_y"
                        fontPath="fonts/vazir.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:paddingBottom="5dp"
                        android:paddingTop="5dp"
                        android:text=" 00 : 00 "
                        android:textColor="@color/black"
                        android:textSize="14sp"
                        tools:ignore="MissingPrefix" />
                </android.support.v7.widget.CardView>

                <TextView
                    android:id="@+id/ta_y"
                    fontPath="fonts/vazir.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="8dp"
                    android:text="تا"
                    android:textColor="@color/colorPrimary"
                    android:textSize="14sp"
                    card_view:layout_constraintRight_toLeftOf="@id/card_view_y"
                    card_view:layout_constraintTop_toTopOf="@id/az_y"
                    tools:ignore="MissingPrefix" />

                <android.support.v7.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:background="@color/color_card_view"
                    card_view:cardCornerRadius="5dp"

                    card_view:cardElevation="5dp"
                    card_view:layout_constraintRight_toLeftOf="@id/ta_y"
                    card_view:layout_constraintTop_toTopOf="@id/az_y">

                    <TextView
                        android:id="@+id/t2_y"
                        fontPath="fonts/vazir.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:text=" 00 : 00 "
                        android:textColor="@color/black"
                        android:textSize="14sp"
                        tools:ignore="MissingPrefix" />
                </android.support.v7.widget.CardView>


            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:background="@color/white"
                android:paddingBottom="8dp"
                card_view:layout_constraintLeft_toLeftOf="@id/afternoon"
                card_view:layout_constraintRight_toRightOf="@id/afternoon"
                card_view:layout_constraintTop_toBottomOf="@id/afternoon">

                <TextView
                    android:id="@+id/az1_y"
                    fontPath="fonts/vazir.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="8dp"
                    android:text="از"
                    android:textColor="@color/colorPrimary"
                    android:textSize="14sp"
                    card_view:layout_constraintRight_toRightOf="parent"
                    card_view:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingPrefix" />

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view1_y"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:background="@color/color_card_view"
                    card_view:cardCornerRadius="5dp"

                    card_view:cardElevation="5dp"
                    card_view:layout_constraintRight_toLeftOf="@id/az1_y"
                    card_view:layout_constraintTop_toTopOf="@id/az1_y">

                    <TextView
                        android:id="@+id/t11_y"
                        fontPath="fonts/vazir.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:text=" 00 : 00 "
                        android:textColor="@color/black"
                        android:textSize="14sp"
                        tools:ignore="MissingPrefix" />
                </android.support.v7.widget.CardView>

                <TextView
                    android:id="@+id/ta1_y"
                    fontPath="fonts/vazir.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="8dp"
                    android:text="تا"
                    android:textColor="@color/colorPrimary"
                    android:textSize="14sp"
                    card_view:layout_constraintRight_toLeftOf="@id/card_view1_y"
                    card_view:layout_constraintTop_toTopOf="@id/az1_y"
                    tools:ignore="MissingPrefix" />

                <android.support.v7.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:background="@color/color_card_view"
                    card_view:cardCornerRadius="5dp"

                    card_view:cardElevation="5dp"
                    card_view:layout_constraintRight_toLeftOf="@id/ta1_y"
                    card_view:layout_constraintTop_toTopOf="@id/az1_y">

