        <TextView
            android:id="@+id/txt2"
            fontPath="fonts/vazir.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:text="دوشنبه"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toLeftOf="@id/check2"
            tools:ignore="MissingPrefix" />

        <CheckBox
            android:id="@+id/check3"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="6dp"
            android:buttonTint="@color/white"
            android:checked="true"
            android:textColor="@color/white"
            android:theme="@style/CheckBoxTheme"
            app:layout_constraintRight_toLeftOf="@id/txt2" />

        <TextView
            android:id="@+id/txt3"
            fontPath="fonts/vazir.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:text="سه شنبه"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toLeftOf="@id/check3"
            tools:ignore="MissingPrefix" />

        <CheckBox
            android:id="@+id/check4"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="6dp"
            android:buttonTint="@color/white"
            android:checked="true"
            android:textColor="@color/white"
            android:theme="@style/CheckBoxTheme"
            app:layout_constraintRight_toLeftOf="@id/txt3" />

        <TextView
            android:id="@+id/txt4"
            fontPath="fonts/vazir.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:text="چهارشنبه"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toLeftOf="@id/check4"
            tools:ignore="MissingPrefix" />

        <CheckBox
            android:id="@+id/check5"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="6dp"
            android:buttonTint="@color/white"
            android:checked="true"
            android:textColor="@color/white"
            android:theme="@style/CheckBoxTheme"
            app:layout_constraintRight_toLeftOf="@id/txt4" />

        <TextView
            android:id="@+id/txt5"
            fontPath="fonts/vazir.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:text="پنجشنبه"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toLeftOf="@id/check5"
            tools:ignore="MissingPrefix" />

        <CheckBox
            android:id="@+id/check6"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="6dp"
            android:buttonTint="@color/white"
            android:checked="true"
            android:textColor="@color/white"
            android:theme="@style/CheckBoxTheme"
            app:layout_constraintRight_toLeftOf="@id/txt5" />

        <TextView
            android:id="@+id/txt6"
            fontPath="fonts/vazir.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:paddingTop="4dp"
            android:text="جمعه"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toLeftOf="@id/check6"
            tools:ignore="MissingPrefix" />


    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/hours_of_office_attendance_txt"
        fontPath="fonts/vazir.ttf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="ساعت مناسب جهت تماس تلفنی کاربران"
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        tools:ignore="MissingPrefix" />

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="12dp"
        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp">

        <android.support.constraint.ConstraintLayout
